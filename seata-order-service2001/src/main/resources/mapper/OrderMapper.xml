<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >

<mapper namespace="com.lyf.springcloud.alibaba.dao.OrderDao">

    <insert id="create">
        insert into t_order (id,product_id,total_amount,status)
        values (null,#{productId},#{totalAmount},0);
    </insert>

</mapper>


